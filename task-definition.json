{
    "family": "api-festivos-task-definition",
    "networkMode": "awsvpc",
    "cpu": "256",  
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::829760431781:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "container-api-festivos-tt",
        "image": "829760431781.dkr.ecr.us-east-2.amazonaws.com/api-festivos-tt",
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/api-festivos-tt",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    }
  }
